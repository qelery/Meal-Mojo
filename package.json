{
  "name": "meal-mojo-angular-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:all": "npm test && npm run test:ui:ci",
    "test:unit": "ng test",
    "test:ui:gui": "concurrently \"ng serve --port 4201 --configuration test\" \"wait-on http-get://localhost:4201 && npm run cy:open\" --kill-others --success first",
    "test:ui:chi": "concurrently \"ng serve --port 4201 --configuration test\" \"wait-on http-get://localhost:4201 && npm run cy:run\" --kill-others --success first",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint": "ng lint",
    "eslint": "./node_modules/.bin/eslint ./",
    "eslint:fix": "./node_modules/.bin/eslint ./ --fix",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^12.2.3",
