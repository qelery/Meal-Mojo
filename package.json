{
  "name": "meal-mojo-angular-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:all": "npm run test && npm run test:ui:cli",
    "test:ui:gui": "concurrently \"ng serve --port 4201 --configuration test\" \"wait-on http-get://localhost:4201 && cypress open\" --kill-others --success first",
    "test:ui:ch": "concurrently \"ng serve --port 4201 --configuration test\" \"wait-on http-get://localhost:4201 && cypress run\" --kill-others --success first",
    "test:unit": "ng test",
    "lint": "ng lint",
    "eslint": "./node_modules/.bin/eslint ./",
    "eslint:fix": "./node_modules/.bin/eslint ./ --fix",
    "e2e": "ng e2e",
    "pretty": "prettier --write \"./src/app/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/cli": "^13.1.2",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/google-maps": "^13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@larscom/ngrx-store-storagesync": "^13.0.6",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "chromedriver": "^97.0.0",
    "moment": "^2.29.1",
    "path": "^0.12.7",
    "rxjs": "~7.5.1",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/compiler-cli": "~13.1.1",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "concurrently": "^7.0.0",
    "cypress": "^9.2.0",
    "enhanced-resolve": "^5.8.3",
    "eslint": "^8.6.0",
    "eslint-plugin-ngrx": "2.0.3",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-spec-reporter": "^0.0.32",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "2.5.1",
    "wait-on": "^6.0.0"
  }
