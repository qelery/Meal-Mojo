    "moment": "^2.29.1",
    "path": "^0.12.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "typescript": "~4.2.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-eslint/builder": "12.3.1",
